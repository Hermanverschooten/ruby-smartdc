#!/usr/bin/env ruby

$LOAD_PATH.unshift(File.join(File.dirname(__FILE__), '..', 'lib'))
require 'cli_helper'

=begin
key = {}

name = ARGV[0]
if name
  key[:name] = name
else
  puts 'name is empty.'
  exit 1
end

key_file = ARGV[1]
if key_file
  begin
    key[:key] = File.read(key_file)
  rescue => e
    puts e
    exit 1
  end
else
  puts 'key file is empty.'
  exit 1
end
=end

aaa = {
  foo: "bar",
  group: "test"
}

#pp client.machines(ARGV[0]).tags.create aaa